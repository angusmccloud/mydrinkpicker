# Copy this file to .env.local and fill in values.
# npm run start now sources .env.local before launching CRA.

# App environment
NODE_ENV=development

# Frontend runtime flags (must be prefixed with REACT_APP_ to be available in browser code)
REACT_APP_ENV=local
REACT_APP_REGION=us-east-1
REACT_APP_APP_ID=d29pkywbp1ulxz

# Optional API overrides (leave blank if amplify_outputs.json is the source of truth)
REACT_APP_GRAPHQL_ENDPOINT=

# Optional logging/debug flags
REACT_APP_ENABLE_DEBUG=false

# Backend referenceAuth values used by Amplify Gen2 deploy/build (not required for local frontend runtime)
AMPLIFY_USERPOOL_ID=
AMPLIFY_USERPOOL_CLIENT_ID=
AMPLIFY_IDENTITYPOOL_ID=
AMPLIFY_AUTH_ROLE_ARN=
AMPLIFY_UNAUTH_ROLE_ARN=
